    "progress": "^2.0.3",
    "prompts": "^2.4.2",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "zod": "^3.23.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.2",
    "@tsconfig/recommended": "^1.0.6",
    "@types/progress": "^2.0.7",
    "@types/prompts": "^2.4.9",
    "@types/socket.io-client": "^3.0.0",
    "i": "^0.3.7",
    "npm": "^10.7.0",
    "typescript": "^5.4.5"
  },
  "resolutions": {
    "@langchain/core": "0.1.5"
