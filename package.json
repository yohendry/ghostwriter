{
  "name": "GhostWriter",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "npx tsx ./src/index.ts",
    "serve": "npx tsx ./src/server.ts",
